                                    },
                                    "observations": {
                                        "description": "Observações",
                                        "type": "string",
                                        "nullable": true
                                    },
                                    "purchase_date": {
                                        "description": "Data de compra",
                                        "type": "string",
                                        "format": "date"
                                    },
                                    "attachments[]": {
